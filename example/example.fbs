namespace example;

union Action { Rez, Collision}

struct Vec3 {
    X:float64;
    Y:float64;
    Z:float64;
}

table Rez {
    id:int64;
    obj_ids:[int64];
    objes:[Vec3];
    name:string;
}

table Collision {
    id:int64;
}


struct Time {
    unix:int64;
    nano:int64;
}

table Root {
    current_at:Time;
    action:Action;
}